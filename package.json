{
  "name": "pastel-evening-theme",
  "displayName": "Pastel Evening Theme",
  "description": "Dark theme with pastel colors and a slight blue hint to the editor.",
  "publisher": "kaiusl",
  "version": "0.4.0",
  "icon": "icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaiusl/pastel_evening_vscode.git"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "Theme",
    "Dark",
    "Pastel",
    "Evening"
  ],
  "galleryBanner": {
    "color": "#292b33",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "Pastel Evening Dark",
        "uiTheme": "vs-dark",
        "path": "./dist/themes/pastel_evening_dark.json"
      }
    ],
    "markdown.previewStyles": [
      "./dist/mdstyle/pastel_evening_dark.css"
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "ts-node src/scripts/generate_theme.ts",
    "watch": "nodemon --exec 'npm run build'",
    "clean": "rm -r dist",
    "clean_node": "rm -rf node_modules",
    "web-test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=./",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
